import stdlib

# if, else if, else

var x = 10

if x == 10 {
	print(integer: 1)
}
else if x == 11 {
	print(integer: 2)
}
else if x == 35 {
	print(string: "lorem ipsum")
}
else {
	print(integer: 33)
}


# switch case

switch x {
	# več pogojev skupaj
	case 1, 2, 3:
		print(string: "ena, dva ali tri")
		break
	case 10:
		print(string: "x = 10")
	case 11:
		print(string: "x = 11")
		break
	default:
		print(string: "Ce ni breaka se izvedem tudi jaz")
}


# while

x = 0
while x < 100 {
	if x % 2 == 0 {
		x = x + 5
		print(integer: x)
		continue
	}
	else if x == 50 {
		break
	}
	x = x + 1
}


# for

for i in [1, 2, 5, 10, 100, 1000] {
	print(integer: i)
}

