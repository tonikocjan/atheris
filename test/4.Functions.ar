# Deklaracija funkcij

func test() {
	print("ta funkcija nima nic argumentov in vraca Void")
}

# Funkcija z enim argumentom tipa Bool, ki vraƒça Double
func foo(x: Bool) Double {
	if x {
		return 10.0
	}
	else {
		return 50.0
	}
}

test()
print(foo(x: true))

# Metode z istim imenom

func metoda() {
	print("metodaA")
}

func metoda(x: Int) {
	print("metodaB")
}

func metoda(double: Double) {
	print("metodaC")
}

func metoda(x: Int, y: Bool) {
	print("metodaD")
}

metoda()
metoda(x: 10)
metoda(double: 1.5)
metoda(x: 0, y: false)