import stdlib

class Point {
	var x: Int = 0
	var y: Int = 0

	init(x: Int, y: Int) {
		self.x = x
		self.y = y
	}
}

class Rect {
	var origin: Point
	var size: Point

	init() {
		self.size = Point()
		self.origin = Point()
	}

	init(origin: Point, size: Point) {
		self.origin = origin
		self.size = size
	}
}

