import stdlib

class A {
	func a() {
		print(string: "A::a()")
	}

	let A_a: Int = 10

	func foo() {
		print(string: "A::foo()")
		self.a()
		self.joze()
	}
}

class B: A {
	
	let B_b: Int = 33

	override func a() {
		print(string: "B::a()")
	}

	func b() {
		print(string: "B::b()")
	}
}

class C: B {
	override func a() {
		print(string: "C::a()")
	}

	let C_c: Int = 100
}

