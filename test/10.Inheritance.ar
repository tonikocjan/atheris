import stdlib

class A {
	func a() {
		print(string: "A::a()")
	}

	func foo() {
		print(string: "A::foo()")	
	}
}

class B: A {
	override func a() {
		print(string: "B::a()")
	}

	func b() {
		print(string: "B::b()")
	}
}

class C: B {
	override func a() {
		print(string: "C::a()")
	}
}

class D {
}

var a: A
let x = 30

if x == 10 {
	a = A()
}
else if x == 20 {
	a = B()
}
else {
	a = C()
}

a.a()

C().foo()
C().b()

